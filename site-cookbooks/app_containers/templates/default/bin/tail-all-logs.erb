#!/bin/bash
multitail --label "NGINX > " -n 5 -ci red <%=@home_dir %>/var/log/nginx/access.log  --label "CACHE > " -n 5 -ci blue -I <%=@home_dir %>/var/log/nginx/cache-access.log --label "APACHE> " -n 5 -ci green -I <%=@home_dir %>/var/log/apache2/access.log --label "NGINX > " -n 5 -ci red <%=@home_dir %>/var/log/nginx/error.log --label "APACHE>" -n 5 -ci green -I <%=@home_dir %>/var/log/apache2/error.log